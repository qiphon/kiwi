{
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/kiwi-cli/dist/index.js",
            "args": [
                "--extract",
                "${workspaceFolder}/kiwi-cli/views/carbon/request.ts",
                "--prefix",
                "I18N.TEST"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
} // A launch configuration that compiles the extension and then opens it inside a new window